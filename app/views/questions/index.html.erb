<% content_for(:title, 'StackOverflow') %>

<h1>Index Page</h1>

<% @questions.each do |question| %>
  <div>
    <%= link_to question.title, question_path(question) %></a>
    <p>asked <%= time_ago_in_words(question.created_at) %> ago by <%= link_to question.user.name, user_path(question.user.id) %></a></p>
    <br />
  </div>
<% end %>


